{
	"apiVersion": 2,
	"name": "tab-group/tabs",
	"version": "0.1.0",
	"title": "Tab Layout",
	"category": "design",
	"icon": "index-card",
	"description": "Display content in a tabbed layout",
	"supports": {
		"html": false
	},
	"keywords": [ "tab", "tabbed content"],
	"attributes":{
		"tabLabelsArray":{
			"type": "array",
			"default": []
		},
		"updateChild":{
			"type": "boolean",
			"default": false
		},
		"tabLayout":{
			"enum": [ "top", "left" ],
			"default": "top"
		}
	},
	"textdomain": "tab-group",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
